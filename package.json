{
  "name": "vite-mf-hmr-poc",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:all": "bun install && cd host && bun install && cd ../remote && bun install",
    "dev:remote": "cd remote && pnpm run dev",
    "dev:host": "cd host && pnpm run dev",
    "dev": "concurrently \"pnpm run dev:remote\" \"pnpm run dev:host\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
